{
  "name": "rheactor-models",
  "description": "A collection of models",
  "scripts": {
    "lint": "standard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "NODE_ENV=testing mocha --reporter=dot --timeout=1350 test/\\*.js",
    "test:coverage": "NODE_ENV=testing istanbul cover _mocha test/\\*\\*/\\*.js",
    "test:coverage-travis": "NODE_ENV=testing istanbul cover _mocha --report lcovonly -- -R dot --timeout=1350 test/\\*.js && cat ./coverage/lcov.info | codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ResourcefulHumans/rheactor-models.git"
  },
  "author": "Resourceful Humans <info@resourceful-humans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ResourcefulHumans/rheactor-models/issues"
  },
  "homepage": "https://github.com/ResourcefulHumans/rheactor-models#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "lodash": "^4.12.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.0",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.4",
    "jsonwebtoken": "^7.1.9",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "semantic-release": "^6.3.2",
    "standard": "^8.3.0",
    "validate-commit-msg": "^2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  }
}
